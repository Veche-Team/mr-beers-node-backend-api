generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Beverages {
  UID String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name  String
  type  String
  price Float
  description String  
  volume Float      
  alcPercentage Float
  imagePath String?

   @@unique(fields: [name])
}

model Snacks {
  UID String @id @default(uuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name  String
  type  String
  price Float
  description String
  imagePath String?

   @@unique(fields: [name])
}

